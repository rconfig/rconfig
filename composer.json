{
    "name": "rconfig/rconfig",
    "type": "project",
    "description": "rConfig V8 Core",
    "version": "8.0.0",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4|^8.3|^8.2",
        "fakerphp/faker": "^1.19",
        "guzzlehttp/guzzle": "^7.6",
        "http-interop/http-factory-guzzle": "^1.0",
        "jfcherng/php-diff": "^6.15",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.7",
        "laravel/passport": "^13.1.0",
        "laravel/prompts": "^0.3.6",
        "laravel/sanctum": "^4.0.2",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "lcobucci/clock": "^3.2.0",
        "lcobucci/jwt": "^5.2.0",
        "lorisleiva/cron-translator": "^0.4.2",
        "maatwebsite/excel": "^3.1",
        "markhilton/monolog-mysql": "^0.1.6",
        "nunomaduro/termwind": "^2.0.1",
        "opcodesio/log-viewer": "^3.11",
        "phpseclib/phpseclib": "~3.0",
        "predis/predis": "^3.2.0",
        "spatie/cpu-load-health-check": "^1.0",
        "spatie/laravel-activitylog": "^4.5",
        "spatie/laravel-health": "^1.8",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-query-builder": "^6.2",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.1",
        "beyondcode/laravel-dump-server": "^2.0",
        "larastan/larastan": "^3.5",
        "laravel/pint": "^1.10",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^8.1.1",
        "phpunit/php-code-coverage": "^12.3.1",
        "phpunit/phpunit": "^12.2.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Http/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 6000,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
